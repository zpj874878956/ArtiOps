# ArtiOps环境变量配置
# 开发环境配置

# 调试模式
DEBUG=True

# 安全设置
SECRET_KEY=django-insecure-key-for-development-only
ALLOWED_HOSTS=localhost,127.0.0.1
CORS_ALLOWED_ORIGINS=http://localhost:8080,http://127.0.0.1:8080

# 数据库配置 - SQLite (开发环境)
# 如果你需要使用MySQL，请取消下面的注释并调整参数
DB_NAME=artiops
DB_USER=test
DB_PASSWORD=123456
DB_HOST=localhost
DB_PORT=3306

# Redis配置
# 如果你不需要Redis，可以注释掉这些配置
# REDIS_HOST=localhost
# REDIS_PORT=6379
# REDIS_DB=0
# REDIS_PASSWORD=

# Celery配置
# 如果你不需要Celery，可以注释掉这些配置
# CELERY_BROKER_URL=redis://localhost:6379/0
# CELERY_RESULT_BACKEND=redis://localhost:6379/0

# 加密设置
ENCRYPTION_KEY=insecure-key-for-development-only

# 文件存储
MEDIA_ROOT=./mediafiles
STATIC_ROOT=./staticfiles

# 日志设置
LOG_LEVEL=INFO
LOG_DIR=./logs 